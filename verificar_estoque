def verificar_estoque(produtos):
    alto = 0
    medio = 0
    baixo = 0

    for nome, quantidade in produtos.items():
        if quantidade > 10:
            print(f"{nome}: Estoque ALTO")
            alto += 1
        elif 5 <= quantidade <= 10:
            print(f"{nome}: Estoque MÃ‰DIO")
            medio += 1
        else:
            print(f"{nome}: Estoque BAIXO")
            baixo += 1

    print("\nResumo:")
    print("Produtos com estoque ALTO:", alto)
    print("Produtos com estoque MÃ‰DIO:", medio)
    print("Produtos com estoque BAIXO:", baixo)


# DicionÃ¡rio com os produtos e suas quantidades
estoque = {
    "Mouse Gamer": 12,
    "Teclado MecÃ¢nico": 8,
    "Notebook Pro": 3,
    "Webcam HD": 15,
    "Headset Bluetooth": 4,
    "Cabo HDMI": 6
}

verificar_estoque(estoque)



